<!doctype html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <link rel="icon" type="image/x-icon" href="{{ 'fennie_icon.png' | asset_url }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    <meta property="og:title" content="{{ page_title }}">
    <meta property="og:site_name" content="{{ shop.name }}">
    <meta property="og:url" content="{{ request.path }}">
    <meta property="og:description" content="{{ page_description | escape }}">
    <meta property="og:type" content="website">
    <meta property="og:image" content="{{ 'fennie_og.png' | asset_url }}">

    {{ content_for_header }}
    {{ 'https://unpkg.com/aos@2.3.4/dist/aos.css' | stylesheet_tag }}
    <!-- Header hook for plugins -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>
    {% style %}
      @font-face {
        font-family: "Inter Display";
        font-style: normal;
        font-weight: 100 900;
        font-display: swap;
        src: url("{{ 'InterVariable.ttf' | asset_url }}") format("truetype-variations");
      }
      @font-face {
        font-family: Farmhouse;
        font-style: normal;
        font-weight: 100 900;
        font-display: swap;
        src: url("{{ 'Farmhouse.ttf' | asset_url }}") format("truetype-variations");
      }
      @font-face {
        font-family: "Frank Ruhl Libre";
        font-style: normal;
        font-weight: 100 900;
        font-display: swap;
        src: url("{{ 'FrankRuhlLibre.ttf' | asset_url }}") format("truetype-variations");
      }
    {% endstyle %}
    {{ 'application.css' | asset_url | stylesheet_tag }}
    {{ 'global.css' | asset_url | stylesheet_tag }}
  </head>
  <body>
    <div id="info">
        <p>
            LAUNCHING SOON ON KICKSTARTER
        </p>
    </div>

    <nav id="nav" class="section">
      <div class="container nav-layout">
        <div class="nav-menus">
          <a href="">
            <img src="{{ 'nav-logo.png' | asset_url }}" alt="" class="nav-logo">
          </a>
          <div class="menu-group" id="menuGroup">
            <div id="navlink" onclick="scrollToSection('hero', true)" class="menu-link"> Home </div>
            <div id="navlink" onclick="scrollToSection('about', true)" class="menu-link"> About </div>
            <div id="navlink" onclick="scrollToSection('shop', true)" class="menu-link"> Shop </div>
            <div id="navlink" onclick="scrollToSection('contact', false)" class="menu-link"> Contact </div>
          </div>
        </div>
        <div class="btn-icon-close" id="menuToggle">

          <svg class="icon-hum close" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
            <path d="M4 6H20M4 12H20M4 18H20" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>

          <svg class="icon-close" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
            <path d="M6 18L18 6M6 6L18 18" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>
        </div>
      </div>
    </nav>

    <main role="main">
      {{ content_for_layout }}
    </main>

    {% section 'section-footer' %}
    
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    {{ 'application.js' | asset_url | script_tag }}
    <script src="https://unpkg.com/aos@2.3.4/dist/aos.js"></script>
    <script>
      document.addEventListener("DOMContentLoaded", function() {
        AOS.init({
          duration: 800,  
          once: true       
        });
      });
    </script>

  </body>
</html>
